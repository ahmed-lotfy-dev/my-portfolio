{
  "compilerOptions": {
    "target": "es2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ],
      "@/components/*": [
        "src/app/[locale]/components/*"
      ],
      "@/lib/*": [
        "src/lib/*"
      ],
      "auth": [
        "./auth.ts"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/app/[locale]/dashboard/aside.jsx",
    "src/app/[locale]/components/Skills.tsx",
    ".pagesOld/api/contact.ts",
    "src/app/actions/page.tsx",
    "src/app/[locale]/components/accordion.tsx",
    "src/app/page.tsx",
    "src/db/migrate.ts",
    "src/app/redux/features/Projects",
    "src/db/db-seed-data/prjects-data.ts",
    ".next/dev/types/**/*.ts",
    ".next\\dev/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
