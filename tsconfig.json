{
  "compilerOptions": {
    "target": "es2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "@/components/*": ["src/app/components/*"],
      "@/lib/*": ["src/app/lib/*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/app/dashboard/aside.jsx",
    "src/app/components/Skills.tsx",
    ".pagesOld/api/contact.ts",
    "src/app/actions/page.tsx",
    "src/app/components/accordion.tsx",
    "src/app/page.tsx",
    "src/db/migrate.ts",
    "src/app/redux/features/Projects"
  ],
  "exclude": ["node_modules"]
}
