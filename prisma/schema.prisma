datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

// generator client {
//   provider = "prisma-client-js"
// }
generator client {
  provider = "prisma-client-js"
}

model Certificate {
  id String @id @default(auto()) @map("_id") @db.ObjectId

  certTitle     String
  certDesc      String
  courseLink    String
  certProfLink  String
  certImageLink String

  @@map("certificates")
}

model Project {
  id String @id @default(auto()) @map("_id") @db.ObjectId

  projTitle     String
  projDesc      String
  projRepoLink  String
  projLiveLink  String
  projImageLink String
  tags          String[]

  @@map("projects")
}

